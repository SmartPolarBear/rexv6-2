{
    "terminal.integrated.fontFamily": "Meslo LG M DZ for Powerline",
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "terminal.integrated.fontSize": 12,
    "files.associations": {
        "types.h": "c",
        "traps.h": "c",
        "x86.h": "c",
        "mmu.h": "c",
        "memlayout.h": "c",
        "proc.h": "c",
        "mp.h": "c",
        "buf.h": "c",
        "file.h": "c",
        "user.h": "c",
        "fcntl.h": "c",
        "string.h": "c",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "array": "cpp",
        "deque": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "fstream": "cpp",
        "new": "cpp",
        "optional": "cpp",
        "sstream": "cpp",
        "tuple": "cpp",
        "utility": "cpp",
        "algorithm": "cpp",
        "param.h": "c",
        "defs.h": "c",
        "kalloc.h": "c"
    },
    "autoHeader": {
        "format": {
            "startWith": "/**",
            "middleWith": "*",
            "endWith": "*/",
            "headerPrefix": "@",
        },
        "header": {
            "Author": "SmartPolarBear",
            "Create Time": {
                "type": "createTime",
                "format": "YYYY-MM-DD HH:mm:ss",
            },
            "Modified by": {
                "type": "modifier",
                "value": "SmartPolarBear",
            },
            "Modified time": {
                "type": "modifyTime",
                "format": "YYYY-MM-DD HH:mm:ss"
            },
            "Description": "",
        }
    }
}